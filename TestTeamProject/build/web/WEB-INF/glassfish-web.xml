<glassfish-web-app error-url="">
  <security-role-mapping>
    <role-name>Admin</role-name>
    <group-name>asadmin</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>Emp</role-name>
    <group-name>asadmin</group-name>
    <group-name>managerEmp</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>Item</role-name>
    <group-name>asadmin</group-name>
    <group-name>managerItem</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>Customer</role-name>
    <group-name>managerCustomer</group-name>
    <group-name>asadmin</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>Salgrade</role-name>
    <group-name>managerSalgrade</group-name>
    <group-name>asadmin</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>Ord</role-name>
    <group-name>managerOrd</group-name>
    <group-name>asadmin</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>Dept</role-name>
    <group-name>managerDept</group-name>
    <group-name>asadmin</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>Product</role-name>
    <group-name>managerProduct</group-name>
    <group-name>asadmin</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>Price</role-name>
    <group-name>managerPrice</group-name>
    <group-name>asadmin</group-name>
  </security-role-mapping>
  <class-loader delegate="true"/>
  <jsp-config>
    <property name="keepgenerated" value="true">
      <description>Keep a copy of the generated servlet class' java code.</description>
    </property>
  </jsp-config>
</glassfish-web-app>
